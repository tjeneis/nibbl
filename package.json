{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ci": "pnpm test:unit && pnpm test:nuxt && pnpm test:e2e",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/eslint": "1.9.0",
    "@nuxtjs/i18n": "^10.1.1",
    "@nuxtjs/supabase": "^2.0.1",
    "@supabase/supabase-js": "^2.76.1",
    "@types/node": "^24.9.1",
    "chart.js": "^4.5.1",
    "echarts": "^6.0.0",
    "nuxt": "^4.1.3",
    "nuxt-echarts": "^1.0.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.7",
    "vuetify-nuxt-module": "^0.18.8"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.2",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "jsdom": "^25.0.0",
    "playwright-core": "^1.56.1",
    "sass": "^1.93.2",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
